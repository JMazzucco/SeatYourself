
<%= form_for(@restaurant) do |f| %>
	<div class="field">
		<%= f.label :name %><br/>
		<%= f.text_field :name %>
	</div>

	<div class="field">
		<%= f.label :address %><br/>
		<%= f.text_field :address %>
	</div>

	<div class="field">
		<%= f.label :cuisine_type %><br/>
		<%= f.text_field :cuisine_type %>
	</div>

	<div class="field">
		<%= f.label :website %><br/>
		<%= f.text_field :website %>
	</div>

	<div class="actions">
		<%= f.submit %>
	</div>
<% end %>